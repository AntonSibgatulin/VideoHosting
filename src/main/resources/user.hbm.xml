<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.AntonSibgatulin.User">

    <class name="User" table="users">


        <meta attribute="class-description">
            This class contains the user detail.
        </meta>


        <id name="id" type="java.lang.Long" column="id">

        </id>
        <property name="login" column="login" type="string"/>
        <property name="password" column="password" type="string"/>
        <property name="City" column="City" type="string"/>
        <property name="date" column="date" type="java.lang.Long"></property>
        <property name="date_register" column="date_register" type="java.lang.Long"></property>
        <property name="mail" column="Email" type="java.lang.String"></property>
        <property name="setting" column="Setting" type="java.lang.String"></property>
        <property name="ban" column="ban" type="java.lang.Integer"></property>
        <property name="type" column="type" type="java.lang.Integer"></property>

        <property name="name" column="name" type="java.lang.String"></property>
        <property name="surname" column="surname" type="java.lang.String"></property>
        <property name="groupes" column="groupes" type="java.lang.String"></property>
        <property name="auth" column="auth" type="java.lang.String"></property>
        <property name="code" column="code" type="java.lang.String"></property>
        <property name="decode" column="decode" type="java.lang.String"></property>
        <property name="countOfSubscribed" column="countOfSubscribed" type="java.lang.Integer"></property>



    </class>
</hibernate-mapping>
